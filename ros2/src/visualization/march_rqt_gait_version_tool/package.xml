<?xml version="1.0"?>
<package format="3">
  <name>march_rqt_gait_version_tool</name>
  <version>0.0.0</version>
  <description>RQT plugin to switch the used version of gaits at runtime.</description>

  <maintainer email="software@projectmarch.nl">Project March</maintainer>
  <license>TODO</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>rclpy</exec_depend>
  <exec_depend>rqt_gui</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>

  <depend>march_shared_msgs</depend>

  <export>
    <build_type>ament_python</build_type>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
