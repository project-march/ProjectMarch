<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>march_gait_preprocessor</name>
  <version>0.0.0</version>
  <description>A node that preprocesses the found points by the cameras for creating dynamic gaits.</description>
  <maintainer email="software@projectmarch.nl">Project March</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>python3-setuptools</buildtool_depend>

  <depend>geometry_msgs</depend>
  <test_depend>python3-pytest</test_depend>
  <exec_depend>rclpy</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
