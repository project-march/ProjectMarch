<?xml version="1.0"?>
<package format="2">
  <name>march_rqt_note_taker</name>
  <version>0.0.0</version>
  <description>An RQT plugin for monitoring log messages and writing logs.</description>

  <maintainer email="software@projectmarch.nl">Project March</maintainer>
  <author email="o.dehaas@projectmarch.nl">Olav de Haas</author>

  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>rqt_gui</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>

  <test_depend>rosunit</test_depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
