<?xml version="1.0"?>
<package format="2">
  <name>march_rqt_gait_generator</name>
  <version>0.1.0</version>
  <description>rqt plugin to generate gaits for the march-iv exoskeleton</description>

  <maintainer email="software@projectmarch.nl">Project March</maintainer>
  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>march_shared_resources</depend>
  <depend>march_shared_classes</depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>rqt_gui</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>xacro</exec_depend>
  <exec_depend>march_description</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>

  <test_depend>rostest</test_depend>
  <test_depend>rosunit</test_depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
