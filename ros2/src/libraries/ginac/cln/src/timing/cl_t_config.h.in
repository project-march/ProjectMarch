#undef HAVE_GETRUSAGE
/* Define as the type of `who' in getrusage() declaration. */
#undef RUSAGE_WHO_T
/* Define if you have <sys/times.h>. */
#undef HAVE_SYS_TIMES_H


#endif /* _CL_T_CONFIG_H */

