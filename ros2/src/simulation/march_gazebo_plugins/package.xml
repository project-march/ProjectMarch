<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>march_gazebo_plugins</name>
  <version>0.0.0</version>
  <description>The Gazebo plugins of the March simulation</description>
  <maintainer email="software@projectmarch.nl">Project MARCH</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>gazebo</depend>
  <depend>gazebo_ros</depend>
  <depend>march_shared_msgs</depend>
  <depend>yaml-cpp</depend>
  <depend>rclcpp</depend>
  <depend>ament_index_cpp</depend>

  <test_depend>ament_cmake_gtest</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <gazebo_ros gazebo_plugin_path="lib"/>
  </export>
</package>
