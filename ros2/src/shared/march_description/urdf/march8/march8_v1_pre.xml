
<mujoco model="exoskeleton">
  <compiler meshdir="obj-files/v1" angle="degree" autolimits="true"/>
  <size nuser_actuator='1' nuser_sensor='1' nuser_geom='1'/>
  <option timestep='0.0005' iterations='50' solver='PGS' gravity='0 0 -9.81'/>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.05 0.05 0.05" rgb2="0.05 0.05 0.05"
             width="512" height="512"/>
    <texture name="body" type="cube" builtin="flat" mark="cross" width="127" height="1278"
             rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" markrgb="1 1 1"
             random="0.01"/>
    <texture name="grid" type="2d" builtin="checker" mark="edge" markrgb="0 0 0" width="800" height="800"
             rgb1=".9 .9 .9" rgb2=".86 .86 .86"/>
    <texture name="logos" file="obj-files/v1/BackPackLogos.png" type="cube" width="512" height="512"/>

    <material name="grid" texture="grid" texrepeat="1 1" texuniform="false"
              shininess=".2"/>
    <material name="march_cover_inside" rgba=".102 .114 .129 1"
              shininess=".2"/>
    <material name="march_cover_outside" rgba=".102 .114 .129 1"
              shininess=".5"/>
    <material name="march_cover_backpack" rgba=".102 .114 .129 1"
              shininess=".5" texture="logos" texuniform="true"/>
    <material name="march_accent_color" rgba=".898 .294 .114 1"
              shininess=".5"/>
    <material name="march_aluminium" rgba=".827 .827 .827 1"
              shininess=".5"/>
    <material name="march_pilot" rgba=".827 .827 .827 1"/>

    <mesh name="backpack"           file="BackPack.obj"/>
    <mesh name="backpack_accent"    file="BackPackAccent.obj"/>
    <mesh name="backpack_partners"  file="BackPackPartners.obj"/>
    <mesh name="backpack_pilot"     file="BackPackPilot.obj"/>

    <mesh name="L_foot"                 file="FootLeft.obj"/>
    <mesh name="L_foot_pilot"           file="FootLeftPilot.obj"/>
    <mesh name="L_hip"                  file="HipLeft.obj"/>
    <mesh name="L_lower_leg_inside"     file="LowerLegLeftInside.obj"/>
    <mesh name="L_lower_leg_outside"    file="LowerLegLeftOutside.obj"/>
    <mesh name="L_lower_leg_accent"     file="LowerLegLeftAccent.obj"/>
    <mesh name="L_lower_leg_pilot"      file="LowerLegLeftPilot.obj"/>
    <mesh name="L_upper_leg_inside"     file="UpperLegLeftInside.obj"/>
    <mesh name="L_upper_leg_outside"    file="UpperLegLeftOutside.obj"/>
    <mesh name="L_upper_leg_accent"     file="UpperLegLeftAccent.obj"/>
    <mesh name="L_upper_leg_pilot"      file="UpperLegLeftPilot.obj"/>
    
    <mesh name="R_foot"                 scale="1 -1 1"  file="FootLeft.obj"/>
    <mesh name="R_foot_pilot"           scale="1 -1 1"  file="FootLeftPilot.obj"/>
    <mesh name="R_hip"                  scale="1 -1 1"  file="HipLeft.obj"/>
    <mesh name="R_lower_leg_inside"     scale="1 -1 1"  file="LowerLegLeftInside.obj"/>
    <mesh name="R_lower_leg_outside"    scale="1 -1 1"  file="LowerLegLeftOutside.obj"/>
    <mesh name="R_lower_leg_accent"     scale="1 -1 1"  file="LowerLegLeftAccent.obj"/>
    <mesh name="R_lower_leg_pilot"      scale="1 -1 1"  file="LowerLegLeftPilot.obj"/>
    <mesh name="R_upper_leg_inside"     scale="1 -1 1"  file="UpperLegLeftInside.obj"/>
    <mesh name="R_upper_leg_outside"    scale="1 -1 1"  file="UpperLegLeftOutside.obj"/>
    <mesh name="R_upper_leg_accent"     scale="1 -1 1"  file="UpperLegLeftAccent.obj"/>
    <mesh name="R_upper_leg_pilot"      scale="1 -1 1"  file="UpperLegLeftPilot.obj"/>
  </asset>

  <default>
    <site group="5"/>
    <motor ctrlrange="-1 1" ctrllimited="true"/>
    <geom contype="0" conaffinity="0" condim="1" solref="0.005 1"/>
    <equality solref="0.005 1"/>
    <default class="exoskeleton">
      <geom material="march_aluminium" group="2"/>
    </default>
    <default class="collision">
      <geom contype="1" group="3" type="capsule"/>
      <default class="L_collision">
        <geom contype="2" conaffinity="4"/>
      </default>
      <default class="R_collision">
        <geom contype="4" conaffinity="2"/>
      </default>
    </default>
    <default class="HAA_joint">
      <joint damping="${joints/HAA/max_torque / ./max_angvel}"
              armature="${joints/HAA/inertia}"
             frictionloss="${joints/HAA/frictionloss}" stiffness="0"/>
    </default>
    <default class="ADPF_joint">
      <joint damping="${joints/ADPF/max_torque / ./max_angvel}"
              armature="${joints/ADPF/inertia}"
             frictionloss="${joints/ADPF/frictionloss}" stiffness="0"/>
    </default>
    <default class="rotational_joint">
      <joint damping="${joints/HFE/max_torque / ./max_angvel}"
              armature="${joints/HFE/inertia}"
             frictionloss="${joints/HFE/frictionloss}" stiffness="0"/>
    </default>
  </default>

  <visual>
    <map force="0.1" zfar="30"/>
    <rgba haze="0.93 0.25 0.0 1"/>
    <quality shadowsize="4096"/>
    <global offwidth="800" offheight="800"/>
  </visual>

  <worldbody>
    <geom name="floor" size="0 0 .05" type="plane" material="grid" conaffinity="15" condim="3" />
    <light name="spotlight" mode="targetbodycom" target="backpack"
           diffuse=".3 .3 .3" specular="0.1 0.1 0.1" pos="-2 -20 10" cutoff="40"/>

    <body name="backpack"
          pos="0 0 0.95"
          childclass="exoskeleton">
      <!-- <light name="top" pos="0 0 1.5" specular="0.1 0.1 0.1" mode="trackcom"/> -->
      <light name="top2" pos="0 0 30" diffuse="0.5 0.5 0.5" mode="trackcom"/>
      <camera name="diag" pos="3 -2.5 1" xyaxes="1 1 0 0 1 2" mode="trackcom"/>
      <camera name="right" pos="0 -3 1" xyaxes="1 0 0 0 1 2" mode="trackcom"/>
      <site name='imu_backpack' size='0.01' pos="0 0 0"/>
      <inertial pos="0.171 -0.006 0.342" mass="57.096"
                fullinertia="2.931 2.492 1.522 0.023 -0.1 0.025"/>
      <geom type="mesh" mesh="backpack" material="march_cover_outside"/>
      <geom type="mesh" mesh="backpack_accent" material="march_accent_color"/>
      <geom type="mesh" mesh="backpack_partners" material="march_cover_inside"/>
      <geom type="mesh" mesh="backpack_pilot" material="march_pilot"/>
      <geom name="backpack" type="box"
            size="0.051 0.189 0.165"
            pos="-0.02 0 0.13" class="collision"/>

      <site name="imu_torso" size='0.01' pos="0.2 0 0.4"/>
      
      <!--  LEFT LEG -->
      <body name="L_hip" pos="${phys/L_hip/origin/x ./y ./z}">
        <inertial pos="${phys/L_hip/com/x ./y ./z}" mass="${phys/L_hip/mass}"
                  fullinertia="${phys/L_hip/inertia/xx ./yy ./zz ./xy ./xz ./yz}"/>
        <joint name="L_HAA" axis="-1 0 0" range="${joints/HAA/limits/min ./max}" class="HAA_joint"/>
        <geom type="mesh" mesh="L_hip" material="march_cover_outside"/>
        <geom name="L_hip" fromto="0 0 0 ${phys/L_UL/origin/x ./y ./z}"
              size="${phys/L_hip/geom/size}" class="L_collision"/>
        <site name="tor_L_HAA" size="0.01" pos="0 0 0"/>

        <body name="L_UL" pos="${phys/L_UL/origin/x ./y ./z}">
        <inertial pos="${phys/L_UL/com/x ./y ./z}" mass="${phys/L_UL/mass}"
                  fullinertia="${phys/L_UL/inertia/xx ./yy ./zz ./xy ./xz ./yz}"/>
          <joint name="L_HFE" axis="0 -1 0" range="${joints/HFE/limits/min ./max}" class="rotational_joint"/>
          <geom type="mesh" mesh="L_upper_leg_outside" material="march_cover_outside"/>
          <geom type="mesh" mesh="L_upper_leg_inside" material="march_cover_inside"/>
          <geom type="mesh" mesh="L_upper_leg_accent"  material="march_accent_color"/>
          <geom type="mesh" mesh="L_upper_leg_pilot"  material="march_pilot"/>
          <geom name="L_UL" fromto="0 0 0 ${phys/L_LL/origin/x ./y ./z}"
              size="${phys/L_UL/geom/size}" class="L_collision"/>
          <site name="tor_L_HFE" size="0.01" pos="0 0 0"/>

          <body name="L_LL" pos="${phys/L_LL/origin/x ./y ./z}">
            <inertial pos="${phys/L_LL/com/x ./y ./z}" mass="${phys/L_LL/mass}"
                  fullinertia="${phys/L_LL/inertia/xx ./yy ./zz ./xy ./xz ./yz}"/>
            <joint name="L_KFE" axis="0 1 0" range="${joints/KFE/limits/min ./max}" class="rotational_joint"/>
            <geom type="mesh" mesh="L_lower_leg_outside" material="march_cover_outside"/>
            <geom type="mesh" mesh="L_lower_leg_inside" material="march_cover_inside"/>
            <geom type="mesh" mesh="L_lower_leg_accent" material="march_accent_color"/>
            <geom type="mesh" mesh="L_lower_leg_pilot"  material="march_pilot"/>
            <geom name="L_LL" fromto="0 0 0 ${phys/L_foot/origin/x ./y ./z}"
              size="${phys/L_LL/geom/size}" class="L_collision"/>
            <site name="tor_L_KFE" size="0.01" pos="0 0 0"/>

            <body name="L_foot" pos="${phys/L_foot/origin/x ./y ./z}">
              <inertial pos="${phys/L_foot/com/x ./y ./z}" mass="${phys/L_foot/mass}"
                  fullinertia="${phys/L_foot/inertia/xx ./yy ./zz ./xy ./xz ./yz}"/>
              <joint name="L_ADPF" axis="0 -1 0" range="${joints/ADPF/limits/min ./max}" class="ADPF_joint"/>
              <geom type="mesh" mesh="L_foot" material="march_aluminium"/>
              <geom type="mesh" mesh="L_foot_pilot"  material="march_pilot"/>
              <site name="tor_L_ADPF" size="0.01" pos="0 0 0"/>

              <!-- The L_foot geometry models a footplate which extends past the pressure pads on the soles-->
              <geom name="L_foot" type="box" size="${phys/L_foot/geom/size/x ./y ./z}"
                  pos="${phys/L_foot/geom/pos/x ./y ./z}"  class="L_collision"
                  friction="${phys/L_foot/friction}"/>

              <!-- Pressure sole  -->
             <body name="pres_L_0" pos="${phys/L_foot/pads/L_0/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_0" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_0"/>
              </body>
              <body name="pres_L_1" pos="${phys/L_foot/pads/L_1/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_1" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_1"/>
              </body>
              <body name="pres_L_2" pos="${phys/L_foot/pads/L_2/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_2" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_2"/>
              </body>
              <body name="pres_L_3" pos="${phys/L_foot/pads/L_3/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_3" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_3"/>
              </body>
              <body name="pres_L_4" pos="${phys/L_foot/pads/L_4/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_4" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_4"/>
              </body>
              <body name="pres_L_5" pos="${phys/L_foot/pads/L_5/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_5" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_5"/>
              </body>
              <body name="pres_L_6" pos="${phys/L_foot/pads/L_6/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_6" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_6"/>
              </body>
              <body name="pres_L_7" pos="${phys/L_foot/pads/L_7/pos/x ./y phys/L_foot/pads/global/z}">
                <geom name="pres_L_7" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_L_7"/>
              </body>
            </body>
          </body>
        </body>
      </body>

      <!--  RIGHT LEG -->
      <body name="R_hip" pos="${phys/L_hip/origin/x -./y ./z}">
        <inertial pos="${phys/L_hip/com/x -./y ./z}" mass="${phys/L_hip/mass}"
                  fullinertia="${phys/L_hip/inertia/xx ./yy ./zz -./xy ./xz -./yz}"/>
        <joint name="R_HAA" axis="1 0 0" range="${joints/HAA/limits/min ./max}" class="HAA_joint"/>
        <geom type="mesh" mesh="R_hip" material="march_cover_outside"/>
        <geom name="R_hip" fromto="0 0 0 ${phys/L_UL/origin/x -./y ./z}"
              size="${phys/L_hip/geom/size}" class="R_collision"/>
        <site name="tor_R_HAA" size="0.01" pos="0 0 0"/>

        <body name="R_UL" pos="${phys/L_UL/origin/x -./y ./z}">
          <inertial pos="${phys/L_UL/com/x -./y ./z}" mass="${phys/L_UL/mass}"
                  fullinertia="${phys/L_UL/inertia/xx ./yy ./zz -./xy ./xz -./yz}"/>
          <joint name="R_HFE" axis="0 -1 0" range="${joints/HFE/limits/min ./max}" class="rotational_joint"/>
          <geom type="mesh" mesh="R_upper_leg_outside" material="march_cover_outside"/>
          <geom type="mesh" mesh="R_upper_leg_inside" material="march_cover_inside"/>
          <geom type="mesh" mesh="R_upper_leg_accent" material="march_accent_color"/>
          <geom type="mesh" mesh="R_upper_leg_pilot"  material="march_pilot"/>
          <geom name="R_UL" fromto="0 0 0 ${phys/L_LL/origin/x -./y ./z}"
              size="${phys/L_UL/geom/size}" class="R_collision"/>
          <site name="tor_R_HFE" size="0.01" pos="0 0 0"/>

          <body name="R_LL" pos="${phys/L_LL/origin/x -./y ./z}">
            <inertial pos="${phys/L_LL/com/x -./y ./z}" mass="${phys/L_LL/mass}"
                  fullinertia="${phys/L_LL/inertia/xx ./yy ./zz -./xy ./xz -./yz}"/>
            <joint name="R_KFE" axis="0 1 0" range="${joints/KFE/limits/min ./max}" class="rotational_joint"/>
            <geom type="mesh" mesh="R_lower_leg_outside" material="march_cover_outside"/>
            <geom type="mesh" mesh="R_lower_leg_inside" material="march_cover_inside"/>
            <geom type="mesh" mesh="R_lower_leg_accent" material="march_accent_color"/>
            <geom type="mesh" mesh="R_lower_leg_pilot"  material="march_pilot"/>
            <geom name="R_LL" fromto="0 0 0 ${phys/L_foot/origin/x -./y ./z}"
              size="${phys/L_LL/geom/size}" class="R_collision"/>
            <site name="tor_R_KFE" size="0.01" pos="0 0 0"/>

            <body name="R_foot" pos="${phys/L_foot/origin/x -./y ./z}">
              <inertial pos="${phys/L_foot/com/x -./y ./z}" mass="${phys/L_foot/mass}"
                  fullinertia="${phys/L_foot/inertia/xx ./yy ./zz -./xy ./xz -./yz}"/>
              <joint name="R_ADPF" axis="0 -1 0" range="${joints/ADPF/limits/min ./max}" class="ADPF_joint"/>
              <geom type="mesh" mesh="R_foot" material="march_aluminium"/>
              <geom type="mesh" mesh="R_foot_pilot"  material="march_pilot"/>
              <site name="tor_R_ADPF" size="0.01" pos="0 0 0"/>

              <!-- The R_foot geometry models a footplate which extends past the pressure pads on the soles-->
              <geom name="R_foot" type="box" size="${phys/L_foot/geom/size/x ./y ./z}"
                  pos="${phys/L_foot/geom/pos/x -./y ./z}"  class="R_collision"
                  friction="${phys/L_foot/friction}"/>

                  <!-- Pressure sole  -->
              <body name="pres_R_0" pos="${phys/L_foot/pads/L_0/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_0" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_0"/>
              </body>
              <body name="pres_R_1" pos="${phys/L_foot/pads/L_1/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_1" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_1"/>
              </body>
              <body name="pres_R_2" pos="${phys/L_foot/pads/L_2/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_2" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_2"/>
              </body>
              <body name="pres_R_3" pos="${phys/L_foot/pads/L_3/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_3" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_3"/>
              </body>
              <body name="pres_R_4" pos="${phys/L_foot/pads/L_4/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_4" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_4"/>
              </body>
              <body name="pres_R_5" pos="${phys/L_foot/pads/L_5/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_5" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_5"/>
              </body>
              <body name="pres_R_6" pos="${phys/L_foot/pads/L_6/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_6" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_6"/>
              </body>
              <body name="pres_R_7" pos="${phys/L_foot/pads/L_7/pos/x -./y phys/L_foot/pads/global/z}">
                <geom name="pres_R_7" type="sphere" pos="0 0 0"
                      size="${phys/L_foot/pads/global/size}" class="L_collision"  friction="${phys/L_foot/friction}"/>
                <site name="pres_R_7"/>
              </body>
            </body>
          </body>
        </body>
      </body>

    </body>
  </worldbody>

  <actuator>
    <!-- User parameter is the maximum no-load motor RPM -->
    <motor name="left_ankle"    gear="${joints/ADPF/max_torque}"  joint="L_ADPF"  />
    <motor name="left_hip_aa"     gear="${joints/HAA/max_torque}"  joint="L_HAA"  />
    <motor name="left_hip_fe"     gear="${joints/HFE/max_torque}" joint="L_HFE"   />
    <motor name="left_knee"     gear="${joints/KFE/max_torque}"  joint="L_KFE"   />
    <motor name="right_ankle"    gear="${joints/ADPF/max_torque}"  joint="R_ADPF"  />
    <motor name="right_hip_aa"     gear="${joints/HAA/max_torque}"  joint="R_HAA"  />
    <motor name="right_hip_fe"     gear="${joints/HFE/max_torque}" joint="R_HFE"   />
    <motor name="right_knee"     gear="${joints/KFE/max_torque}"  joint="R_KFE" />
  </actuator>

  <sensor>
    <!-- User parameter is the number of absolute encoder bits -->
    <!-- encodersoutput position; connected to the actuator -->
    <jointpos name="L_ADPF_pos_output" joint="L_ADPF" user="18" noise="000"/>
    <jointpos name="L_HAA_pos_output" joint="L_HAA" user="18" noise="000"/>
    <jointpos name="L_HFE_pos_output" joint="L_HFE" user="18" noise="000"/>
    <jointpos name="L_KFE_pos_output" joint="L_KFE" user="18" noise="000"/>
    <jointpos name="R_ADPF_output" joint="R_ADPF" user="18" noise="000"/>
    <jointpos name="R_HAA_output" joint="R_HAA" user="18" noise="000"/>
    <jointpos name="R_HFE_output" joint="R_HFE" user="18" noise="000"/>
    <jointpos name="R_KFE_output" joint="R_KFE" user="18" noise="000"/>
    <!-- encodersoutput velocity; connected to the actuator -->
    <jointvel name="L_ADPF_vel_output" joint="L_ADPF" user="18" noise="000"/>
    <jointvel name="L_HAA_vel_output" joint="L_HAA" user="18" noise="000"/>
    <jointvel name="L_HFE_vel_output" joint="L_HFE" user="18" noise="000"/>
    <jointvel name="L_KFE_vel_output" joint="L_KFE" user="18" noise="000"/>
    <jointvel name="R_ADPF_vel_output" joint="R_ADPF" user="18" noise="000"/> 
    <jointvel name="R_HAA_vel_output" joint="R_HAA" user="18" noise="000"/>
    <jointvel name="R_HFE_vel_output" joint="R_HFE" user="18" noise="000"/>
    <jointvel name="R_KFE_vel_output" joint="R_KFE" user="18" noise="000"/>
    <!-- torque sensors; connected to the actuator-->

<!--    TODO: This could also be of the type actuatorvel -->
    <torque name="L_ADPF_tor_output" site="tor_L_ADPF" user="13" noise="000"/>
    <torque name="L_HAA_tor_output" site="tor_L_HAA" user="13" noise="000"/>
    <torque name="L_HFE_tor_output" site="tor_L_HFE" user="13" noise="000"/>
    <torque name="L_KFE_tor_output" site="tor_L_KFE" user="13" noise="000"/>
    <torque name="R_ADPF_tor_output" site="tor_R_ADPF" user="13" noise="000"/>
    <torque name="R_HAA_tor_output" site="tor_R_HAA" user="13" noise="000"/>
    <torque name="R_HFE_tor_output" site="tor_R_HFE" user="13" noise="000"/>
    <torque name="R_KFE_tor_output" site="tor_R_KFE" user="13" noise="000"/>

    <!-- pressure soles not finished yet -->
